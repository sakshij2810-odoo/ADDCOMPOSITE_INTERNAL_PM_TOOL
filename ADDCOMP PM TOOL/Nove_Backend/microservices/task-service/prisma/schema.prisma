// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model TaskModuleWise {
  task_module_wise_id            Int       @id @default(autoincrement())
  task_module_wise_uuid          String    @unique @default(uuid())
  task_module_wise_code          String    @unique
  module_name                    String
  sub_module_name                String
  module_reference_column        String?
  module_reference_code_or_id    String?
  task_name                      String
  description                    String?
  task_completed_date            DateTime?
  task_priority                  String?
  assigned_to_uuid               String?
  assigned_to_name               String?
  created_by_uuid                String?
  created_by_name                String?
  modified_by_uuid               String?
  modified_by_name               String?
  task_type                      String
  status                         String    @default("ACTIVE")
  file_upload                    Json?
  date_created                   String?
  due_date                       String?
  due_time                       String?
  date_completed                 String?
  time_completed                 String?
  create_ts                      DateTime  @default(now())
  insert_ts                      DateTime  @default(now())

  @@map("task_module_wise")
}